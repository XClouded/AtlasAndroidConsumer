<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="220dp"
    android:layout_alignParentBottom="true"
    android:layout_alignParentLeft="true" >

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:background="@drawable/tabbar_icon_home_selector"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_below="@+id/imageButton1"
        android:layout_toLeftOf="@+id/imageButton1"
        android:background="@drawable/tabbar_icon_calendar_selector"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_below="@+id/imageButton1"
        android:layout_toRightOf="@+id/imageButton1"
        android:background="@drawable/tabbar_icon_tasks_selector"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_below="@+id/imageButton2"
        android:layout_toLeftOf="@+id/imageButton2"
        android:background="@drawable/tabbar_icon_contacts_selector"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_below="@+id/imageButton3"
        android:layout_toRightOf="@+id/imageButton3"
        android:background="@drawable/tabbar_icon_settings_selector"
        android:contentDescription="@string/app_name" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/tabbar_icon_atlas_selector"
        android:contentDescription="@string/app_name" />

</RelativeLayout>